Compression yes
IdentitiesOnly yes

Host *
 AddKeysToAgent yes
 UseKeychain yes

Host github.com
    IdentityFile ~/.ssh/keys/github

Host themacmini.local
    IdentityFile ~/.ssh/keys/themacmini
    User matt

Host themacmini
    HostName themacmini.local
    IdentityFile ~/.ssh/keys/themacmini
    LocalForward 1202 127.0.0.1:5900
    User matt

Host dtk.local
    IdentityFile ~/.ssh/keys/dtk
    User matt

Host dtk
    HostName dtk.local
    IdentityFile ~/.ssh/keys/dtk
    LocalForward 1202 127.0.0.1:5900
    User matt

Host home
    HostName home.massicotte.org
    IdentityFile ~/.ssh/keys/themacmini
    LocalForward 1202 127.0.0.1:5900
    User matt